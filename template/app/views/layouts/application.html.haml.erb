!!!
%html{ "ng-app" => "<%= @AppName %>"}
  %head{ "ng-controller" => "HeadsCtrl" }
    %title {{title}}
    = stylesheet_link_tag "application", :media => 'all'
    = csrf_meta_tags
  %body
    = render "layouts/navbar"
    .container
      %div{ "ng-controller" => "FlashsCtrl" }
        = render "layouts/messages"
      = yield
      = javascript_include_tag 'application'
      = js_env
