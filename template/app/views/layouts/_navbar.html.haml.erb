.navbar.navbar-default.navbar-static-top{ "role" => "navigation", "ng-controller" => "NavBarCtrl" }
  .container
    .navbar-header
      = link_to "<%= @AppName %>", root_path, class: "navbar-brand"
    .navbar-collapse.collapse
      %ul.nav.navbar-nav.navbar-right
        = render 'layouts/admin_menu'
        = render 'layouts/menu'
        = render 'layouts/auth'
